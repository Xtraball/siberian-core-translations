msgstr "Настройки Push-сообщения"

msgid "Push successfully deleted."
msgstr "Пользователь успешно удален"

msgid "Radius"
msgstr "Радиус"

msgid "Send to a specific location"
msgstr "Отправить в определенное место"

msgid "Send to all applications"
msgstr "Послать всем приложениям"

msgid "Send to all my users"
msgstr "Послать всем своим пользователям"

msgid "Send to no specific locations"
msgstr "Отправить в определенное место"

msgid "Send to specific topics"
msgstr "Отправить в определенное место"

msgid "Send to specific users"
msgstr "Отправить определенным пользователям"

